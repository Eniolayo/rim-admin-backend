{
  "name": "rim-admin-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ormconfig.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ormconfig.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ormconfig.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d ormconfig.ts",
    "migration:generate:docker": "docker-compose exec app npm run migration:generate",
    "migration:run:docker": "docker-compose exec app npm run migration:run",
    "migration:revert:docker": "docker-compose exec app npm run migration:revert",
    "migration:show:docker": "docker-compose exec app npm run migration:show",
    "migration:generate:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run migration:generate",
    "migration:run:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run migration:run",
    "migration:revert:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run migration:revert",
    "migration:show:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run migration:show",
    "seed:admin": "ts-node -r tsconfig-paths/register src/database/seeds/admin.seed.ts",
    "seed:admin:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run seed:admin",
    "seed:user": "ts-node -r tsconfig-paths/register src/database/seeds/user.seed.ts",
    "seed:user:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run seed:user",
    "seed:loan": "ts-node -r tsconfig-paths/register src/database/seeds/loan.seed.ts",
    "seed:loan:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run seed:loan",
    "seed:transaction": "ts-node -r tsconfig-paths/register src/database/seeds/transaction.seed.ts",
    "seed:transaction:docker:dev": "docker-compose -f docker-compose.dev.yml exec app npm run seed:transaction"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/ioredis": "^4.28.10",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.63.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ioredis": "^5.8.2",
    "joi": "^17.13.3",
    "nestjs-pino": "^4.1.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pino": "^9.3.0",
    "pino-http": "^10.0.0",
    "pino-pretty": "^11.2.2",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
